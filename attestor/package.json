{
  "name": "attestor-observer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "clean:all": "rm -rf dist && rm -rf node_modules && rm -rf pkg",
    "build": "./build_all.sh",
    "build:ts": "tsc -p .",
    "build:wasm": "AR=/opt/homebrew/opt/llvm/bin/llvm-ar CC=/opt/homebrew/opt/llvm/bin/clang wasm-pack build --target bundler .",
    "run": "NODE_OPTIONS=--experimental-wasm-modules node dist/src/index.js",
    "run:1": "PORT=8801 npm run run",
    "run:2": "PORT=8802 npm run run",
    "run:3": "PORT=8803 npm run run",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"npm run run\"",
    "dev:1": "PORT=8801 npm run dev",
    "dev:2": "PORT=8802 npm run dev",
    "dev:3": "PORT=8803 npm run dev",
    "lint": "npx prettier ./observer --write",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stacks/blockchain-api-client": "^7.3.0",
    "@stacks/common": "^6.5.5",
    "@stacks/network": "^6.5.5",
    "@stacks/transactions": "^6.7.0",
    "@types/node": "^20.5.0",
    "attestor": "file:pkg",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "chalk": "^5.3.0",
    "cross-fetch": "^3.1.8",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "micro-stacks": "^1.2.1",
    "node-fetch": "^3.3.2",
    "prom-client": "^12.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "strftime": "^0.10.2",
    "swagger-stats": "^0.99.7",
    "tiny-secp256k1": "^2.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/strftime": "^0.9.4",
    "@types/swagger-stats": "^0.95.11",
    "tsc-watch": "^6.0.4"
  }
}
